{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "no-empty": [
            true,
            "allow-empty-functions"
        ],
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "jsx-wrap-multiline": false,
        "jsx-no-multiline-js": false
    },
    "rulesDirectory": []
}
